{
	"name": "sentinel",
	"version": "0.2.1-alpha",
	"private": true,
	"license": "Sentinel-License",
	"type": "module",
	"main": "build/main.js",
	"scripts": {
		"build": "tsc",
		"exec": "ts-node-esm src/main.ts",
		"watch": "nodemon --exec npm run exec",
		"test-import": "ts-node-esm src/test-import.ts",
		"lint": "npx eslint src/*.ts",
		"format": "npx prettier --write src/**/*.ts",
		"pre-commit": "npx prettier --write . && npx eslint src/*.ts --fix && npm i"
	},
	"dependencies": {
		"@discordjs/collection": "^1.2.0",
		"@discordjs/voice": "^0.13.0",
		"@discordx/importer": "^1.1.10",
		"@types/node": "^18.11.8",
		"discord.js": "^14.6.0",
		"discordx": "^11.4.0",
		"mongoose": "^6.7.1",
		"pluralize": "^8.0.0",
		"prettier": "^2.7.1",
		"reflect-metadata": "^0.1.13",
		"util": "^0.12.5"
	},
	"devDependencies": {
		"@types/node": "^18.11.8",
		"@types/pluralize": "^0.0.29",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.41.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"nodemon": "^2.0.20",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4",
		"typescript-eslint": "^0.0.1-alpha.0"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	}
}
