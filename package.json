{
	"name": "sentinel",
	"version": "0.3.0-alpha",
	"private": true,
	"license": "Sentinel-License",
	"type": "module",
	"main": "build/src/main.js",
	"scripts": {
		"build": "tsc",
		"start": "ts-node-esm src/main.ts",
		"watch": "tsnodemon src/main.ts",
		"test": "vitest --coverage",
		"lint": "eslint src/*.ts",
		"format": "prettier --write .",
		"generate-badges": "istanbul-badges-readme",
		"pre-commit": "npm ci && rm -rf coverage build && npm run format && eslint src/*.ts --fix && npm test run"
	},
	"dependencies": {
		"@discordjs/collection": "^1.2.0",
		"@discordjs/voice": "^0.14.0",
		"@discordx/importer": "^1.1.10",
		"discord.js": "^14.6.0",
		"discordx": "^11.4.0",
		"lodash-es": "^4.17.21",
		"mongoose": "^6.8.2",
		"nyc": "^15.1.0",
		"pluralize": "^8.0.0",
		"reflect-metadata": "^0.1.13",
		"util": "^0.12.5"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^18.11.18",
		"@types/pluralize": "^0.0.29",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.47.1",
		"@vitest/coverage-istanbul": "^0.26.3",
		"dotenv": "^16.0.3",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.5.0",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.1",
		"ts-node": "^10.9.1",
		"tsnodemon": "^1.2.2",
		"typescript": "^4.9.4",
		"typescript-eslint": "^0.0.1-alpha.0",
		"vitest": "^0.25.3"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	}
}
